<% include ../partials/navbar %>
<div id="bg-1" style="height: 100%;" class="container-fluid text-center">
  <h1>Contact Me</h1>
  <form class="contact-form">
    <div class="form-group">
      <label for="name-contact">Name:</label><br>
      <input type="text" name="name-contact" placeholder="John Doe" required><br>
        <small class="form-text text-muted">Your name will not be shared with anyone else.</small>
    </div>
    <div class="form-group">
      <label for="email-contact">Email Address:</label><br>
      <input type="text" name="email-contact" placeholder="example@example.com" required><br>
      <small class="form-text text-muted">Your email will not be shared with anyone else.</small>
    </div>
    <div class="form-group">
      <label for="details-contact">Details:</label><br>
      <input type="text" name="detail-contact" placeholder="2-3 Sentences" required>
    </div>
  </form>
</div>

